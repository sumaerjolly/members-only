<%= provide(:title, "All Posts") %>
<h1>All Posts </h1>
<% if logged_in? %>
  <%= link_to 'Create Post', new_post_path %>
<% end %><br><br>
<table>
  <% @posts.each do |post| %>
    <div>
      <h4><%= post.title %><%= " by #{User.find(post.user_id).name}" if logged_in? %></h4>
      <p><%= post.body %></p>
    </div>

  <% end %>

</table>

